<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.bake0255.codingtest" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Powershifter Codingtest</name>
    <description>
        I created an static HTML and CSS file using bootstrap CSS framework. I used the PSD file as a template to build my design and code inHTML and CSS.
    </description>
    <author email="lindseyb43@gmail.com" href="https://lindseybaker.myportfolio.com">
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
